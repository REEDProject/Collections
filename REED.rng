               <ref name="placeName"/>
            </optional>
            <optional>
               <ref name="settlement"/>
            </optional>
     
            <optional>
               <ref name="institution"/>
            </optional>
     
     
            <optional>
               <ref name="repository"/>
            </optional>
     
     
            <optional>
               <ref name="collection"/>
            </optional>
     
            <ref name="idno"/>
     
            <optional>
               <ref name="note"/>
