         <ref name="att.datable.attribute.notBefore-custom"/>
         <ref name="att.datable.attribute.notAfter-custom"/>
         <ref name="att.datable.attribute.from-custom"/>
         <ref name="att.datable.attribute.to-custom"/>
         <ref name="att.datable.attribute.datingPoint"/>
         <ref name="att.datable.attribute.datingMethod"/>
         <ref name="att.datable.attribute.period"/>
         <ref name="att.datable.w3c.attribute.when"/>
         <ref name="att.datable.w3c.attribute.notBefore"/>
         <ref name="att.datable.w3c.attribute.notAfter"/>
         <ref name="att.datable.w3c.attribute.from"/>
         <ref name="att.datable.w3c.attribute.to"/>
         <empty/>
