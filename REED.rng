               </optional>
        
               <optional>
                  <ref name="institution"/>
               </optional>
        
        
               <optional>
                  <ref name="repository"/>
               </optional>
        
        
               <zeroOrMore>
                  <ref name="collection"/>
               </zeroOrMore>
        
        
               <optional>
                  <ref name="idno"/>
               </optional>
        
            </group>
      
            <zeroOrMore>
               <choice>
                  <ref name="msName"/>
                  <ref name="altIdentifier"/>
               </choice>
            </zeroOrMore>
      
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="REED-msIdentifier-msId_minimal-constraint-report-20">
