                  <optional>
                     <ref name="rubric"/>
                  </optional>
          
          
                  <optional>
                     <ref name="incipit"/>
                  </optional>
          
          
                  <zeroOrMore>
                     <ref name="msItemStruct"/>
                  </zeroOrMore>
          
          
                  <optional>
