                  <group>
                     <ref name="row"/>
          
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
          
                     <ref name="model.graphicLike"/>
          
          
                     <zeroOrMore>
                        <ref name="model.global"/>
